    padding-right: 50px;
    color: var(--white-color);
}
#ui-id-2 > span.ui-menu-icon.ui-icon.ui-icon-carat-1-e {
    display: none;
}
span.ui-menu-icon.ui-iconu.i-icon-carat-1-e {
