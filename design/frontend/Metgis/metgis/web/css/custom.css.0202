.block .block-banner {
    margin-bottom: 10px !important;
}

/* ------- Begin Contact Us form -----------------------------*/
.form.contact {
    font-size: 14px;
    line-height: 17px;
    text-align: justify;
    position: relative;
}
.form.contact .fieldset {
    border: 0;
    background: #FC0;
    padding: 22px 25px 12px 33px;
    margin: 28px 0;
}
div.field.note.no-label {
    float: left;
    font-weight: bold;
    font-size: 13px;
    border: 0;
    background: var(--blue-color);
    color: var(--white-color);
   /* margin: -53px 0 0 -10px;*/
    padding: 0 8px;
    top: -8px;
    margin-bottom: 0;
    position: absolute;
}
.form-list label {
    float: left;
    color: #666;
    font-weight: bold;
    position: relative;
    z-index: 0;
}
.fieldset .fields {
    display: flex;
    justify-content: space-between;
}
.field.name, .field.email {
    width: 48%;
}
.form-list .required label em {
    float: right;
    font-style: normal;
    color: #eb340a !important;
    /* position: absolute; */
    /* top: 0; */
    /* right: -8px; */
}
.actions-toolbar {
    clear: both;
    margin: 4em 0 0;
    padding: 8px 0 0;
    border-top: 1px solid #e4e4e4;
    text-align: right;
}
p.required {
    font-size: 11px;
    text-align: right;
    color: #EB340A;
}
.primary {
    float: right !important;
}
.primary .submit {
    overflow: visible;
    width: auto;
    border: 0;
    padding: 0 10px;
    margin: 0;
    /* background: transparent; */
    cursor: pointer;
    font: 12px/15px Arial, Helvetica, sans-serif;
    width: auto;
}
textarea#comment{
    height: 124px;
}
/* ------- End Contact Us form -----------------------------*/

