}
.quick-access {
    position: absolute;
    top: 3%;
    right: 0%;
    z-index: 5;
    text-transform: uppercase;
    display: flex;
    color:  #4b3f4b;
    width: fit-content;
}
.quick-access ul {
    padding-left: 5px;
}
.quick-access .block-cms-link {
    margin-top: 0 !important;
}
.quick-access li a {
    color: #4b3f4b;
    text-decoration: none;
    cursor: pointer;
    font-size: 11px;
}
.quick-access ul li span a span {
    padding-left: 5px;
    border-left: 1px solid  #4b3f4b;
}
