.mod-languages {
    display: inline-block;
    position: absolute;
    right: 5%;
    top: 10%;
    z-index: 6;
}

.countries{
    position: absolute;
    top: 8%;
    right: 5%;
    z-index: 5;
}
.top-menu-link {
    position: absolute;
    z-index: 6;
    font-weight: bold;
    font-size: var(--normal);
    top: 98%;
    left: 23%;
}
.top-menu-link a, .top-menu-link a:hover  {
    color: var(--white-color);
