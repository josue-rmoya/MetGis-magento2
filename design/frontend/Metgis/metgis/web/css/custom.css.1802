    ol.widget-new-grid {
        display: initial !important;
    }
    ol.product-items {
        display: inherit !important;
    }
    /*-- sidebar ----*/
    .top-link-checkout {
        background: initial;
    }
    .block-cart .minicart-wrapper {
        float: initial;
        background-color: var(--white-color);
        height: max-content;
        margin-left: 0;
        width: 100%;
    }
